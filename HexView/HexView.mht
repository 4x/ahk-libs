    .CHEntry a:visited,
    .CHEntry a:hover {
        color: #606060;
        }
    .CHEntry a:active {
        color: #800000;
        }





body.FramedIndexPage,
.IndexSection {
    background-color: #FFFFFF;
    font-size: 10pt;
    padding: 15px }

.IndexSection {
    border-width: 0 0 1px 1px; border-style: solid; border-color: #000000 }

    .IPageTitle {
        font-size: 20pt; font-weight: bold;
